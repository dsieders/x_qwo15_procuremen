	bodyText = email.body_html;
current.work_notes = "Procurement Request created by email:\n\nReceived from: " + email.origemail + "\n\n" + email.subject + "\n\n" + bodyText;

// See if there is a PO number in the subject, if so add it to the PO number field.
var regexp = /\b(30|31|32|41)[0-9]{8}\b/;

if(regexp.test(email.subject)){	
	current.x_qwo15_procuremen_u_po_number = (regexp.exec(email.subject))[0];
}
else if(regexp.test(email.bodyText)){
	current.x_qwo15_procuremen_u_po_number = (regexp.exec(bodyText))[0];
}

